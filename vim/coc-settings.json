{
  "snippets.ultisnips.enable": false,
  "signature.preferShownAbove": false,
  "signature.triggerSignatureWait": 1000,
  "signature.floatConfig": {
    "maxHeight": 6,
    "maxWidth": 100
  },
  "suggest.timeout": 500,
  "suggest.maxCompleteItemCount": 6,
  "suggest.snippetsSupport": true,
  "suggest.noselect": true,
  "suggest.disableKind": false,
  "suggest.disableMenu": true,
  "suggest.enablePreview": true,
  "suggest.detailMaxLength": 200,
  "suggest.floatConfig": {
    "border": true,
    "highlight": "Normal",
    "borderhighlight": "Pmenu"
  },
  "suggest.completionItemKindLabels": {
    "keyword": "Z",
    "variable": "v",
    "value": "?",
    "operator": " ",
    "constructor": " ",
    "function": "f",
    "reference": " ",
    "constant": "C",
    "method": "m",
    "struct": "s",
    "class": "c",
    "interface": "I",
    "text": " ",
    "enum": " ",
    "enumMember": " ",
    "module": " ",
    "color": " ",
    "property": ".",
    "field": ".",
    "unit": " ",
    "event": " ",
    "file": "F",
    "folder": "/",
    "snippet": "~",
    "typeParameter": "T",
    "default": " "
  },
  "tsserver.log": "verbose",
  "tsserver.formatOnType": true,
  "tsserver.trace.server": "verbose",
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.statusItemText": "",
  "diagnostic.autoRefresh": true,
  "diagnostic.locationlistUpdate": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.format": "%message",
  "diagnostic.refreshOnInsertMode": true,
  "list.source.diagnostics.defaultAction": "vsplit",
  "list.source.diagnostics.pathFormat": "short",
  "list.source.diagnostics.includeCode": false,
  "list.source.symbols.defaultAction": "vsplit",
  "list.alignColumns": true,
  "outline.sortBy": "position",
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnType": true,
  "coc.preferences.willSaveHandlerTimeout": 500,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "css"
  ],
  "go.goplsEnv": {
    "GOPROXY": "off"
  },
  "go.goplsOptions": {
    "usePlaceholders": true,
    "hoverKind": "SynopsisDocumentation",
    "linksInHover": false,
    "experimentalPackageCacheKey": false
  },
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json", ".git/", ".hg/"],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    },
    "robotframework_ls": {
      "command": "robotframework_ls",
      "args": ["-vv", "--log-file=~/logs/robotframework_ls.log"],
      "filetypes": ["robot"],
      "settings": {
        "robot.pythonpath": ["${env:ROBOTLIBS}"]
      }
    }
  }
}
