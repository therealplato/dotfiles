{
  "suggest.timeout": 500,
  "suggest.maxCompleteItemCount": 5,
  "suggest.noselect": true,
  "suggest.completionItemKindLabels": {
    "keyword": "",
    "variable": "v",
    "value": "",
    "operator": "",
    "constructor": "f",
    "function": "f",
    "reference": "",
    "constant": "C",
    "method": "m",
    "struct": "s",
    "class": "c",
    "interface": "I",
    "text": "",
    "enum": "",
    "enumMember": "",
    "module": "",
    "color": "",
    "property": ".",
    "field": ".",
    "unit": "",
    "event": "",
    "file": "*",
    "folder": "/",
    "snippet": "~",
    "typeParameter": "T",
    "default": ""
  },
  "tsserver.log": "verbose",
  "tsserver.formatOnType": true,
  "tsserver.trace.server": "verbose",
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.statusItemText": "",
  "diagnostic.autoRefresh": true,
  "diagnostic.locationlistUpdate": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.format": "%message",
  "diagnostic.refreshOnInsertMode": true,
  "list.source.diagnostics.defaultAction": "vsplit",
  "list.source.diagnostics.pathFormat": "short",
  "list.source.diagnostics.includeCode": false,
  "list.source.symbols.defaultAction": "vsplit",
  "list.alignColumns": true,
  "outline.sortBy": "position",
  "coc.preferences.useQuickfixForLocations": true,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnType": true,
  "coc.preferences.willSaveHandlerTimeout": 500,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "css"
  ],
  "languageserver": {
    "robotframework_ls": {
      "command": "robotframework_ls",
      "filetypes": ["robot"],
      "settings": {
        "robot.language-server.args": [
          "-vv",
          "--log-file=~/robotframework_ls.log"
        ]
      }
    }
  }
}
